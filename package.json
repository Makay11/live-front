  "name": "live-front",
  "description": "Extract LSON, CSON or JSON from the header of any file. Inspired by front-matter.",
  "keywords": [
    "coffeescript",
    "cson",
    "front-matter",
    "json",
    "livescript",
    "lson"
  ],
  "version": "1.0.0",
  "license": "ISC",
  "homepage": "https://github.com/Makay11/live-front",
  "bugs": "https://github.com/Makay11/live-front/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Makay11/live-front"
  },
  "main": "index.js",
  "scripts": {
    "checks": "npm run jshint && npm test",
    "jshint": "jshint .",
    "start": "npm run watch",
    "test": "jasmine",
    "watch": "nodemon --delay 1 --ext js,json,jade,html --exec npm run checks"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "lson": "^1.0.3"
  },
  "devDependencies": {
    "jasmine": "^2.3.2",
    "jshint": "^2.8.0",
    "nodemon": "^1.4.1"
  }
}
